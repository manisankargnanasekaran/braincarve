                <div class ="col-md-3">
                <div class = "card hovercard">
                <div class = "row  empl-detail ">
                <div class="info">
                <div class="title">
                  <% @franchisee_childs = Franchisee.where(franchisee_type: "National Franchisee(NF)") %>
                  <% @franchisee_childs.map do |each_child| %>
                  
                      <p><%= each_child.location %>
                      (<%= each_child.parent %>) <i  style="color:#8a9906;" class="fa fa-circle"></i></p>

                     
    <!-- priority = OrderSheet.find(order_sheet_id).priority -->
   
                    
                  <%end%>
                  </div>
                </div>
                </div>
                </div>
                </div>
          <div class ="col-md-3">
           <div class = "card hovercard">
                <div class = "row  empl-detail ">
                <div class="info">
                <div class="title">
            <% @franchisee_child = Franchisee.where(franchisee_type: "Master Franchisee(MF)") %>
           
            <% @franchisee_child.map do |each_child| %>
           
                <p><%= each_child.location %>
                <%unless each_child.parent == "BrainCarve" %>

                (<%= each_child.parent %>)<% parent_color = Franchisee.find_by(location: each_child.parent).franchisee_type%>
                <%else%>
                (<%= each_child.parent %>)<% parent_color = each_child.parent %>
                <% end %>
               <%case parent_color %>
                <% when "BrainCarve" %>
                  <i  style="color:#8a9906;" class="fa fa-circle"></i>
                <% when "National Franchisee(NF)" %>
                  <i  style="color:#990707;" class="fa fa-circle"></i>
               <% end %></p>
  
            <%end%>
          </div>
          </div>
          </div>
          </div>
          </div>
          <div class ="col-md-3">
           <div class = "card hovercard">
                <div class = "row  empl-detail ">
                <div class="info">
                <div class="title">
            <% @franchisee_chil = Franchisee.where(franchisee_type: "District Franchisee(DF)") %>
           
            <% @franchisee_chil.map do |each_child| %>
            
                <p><%= each_child.location %>
                <%unless each_child.parent == "BrainCarve" %>

                (<%= each_child.parent %>)<% parent_color = Franchisee.find_by(location: each_child.parent).franchisee_type%>
                <%else%>
                (<%= each_child.parent %>)<% parent_color = each_child.parent%>
                <%end%>
               <%case parent_color %>
                <% when "BrainCarve" %>
                  <i  style="color:#8a9906;" class="fa fa-circle"></i>
                <% when "National Franchisee(NF)" %>
                  <i  style="color:#990707;" class="fa fa-circle"></i>
                <% when "Master Franchisee(MF)" %>
                 <i  style="color:#ef8a07;" class="fa fa-circle"></i>
               <% end %></p>
              
            <%end%>
          </div>
          </div>
          </div>
          </div>
          </div>
          <div class ="col-md-3">
           <div class = "card hovercard">
                <div class = "row  empl-detail ">
                <div class="info">
                <div class="title">
            <% @franchisee_chi = Franchisee.where(franchisee_type: "Unit Franchisee(UF)") %>
            <% @franchisee_chi.map do |each_child| %>
              
                <p><%= each_child.location %>
                (<%= each_child.parent %>)<% parent_color = each_child.parent%>

                <% unless each_child.parent == "BrainCarve" %>

                <!-- (<%#= each_child.parent %>) -->
                <% parent_color = Franchisee.find_by(location: each_child.parent).franchisee_type%>
                <%else%>
                (<%= each_child.parent %>)<% parent_color = each_child.parent%>
                <%end%>
               <%case parent_color %>
                <% when "BrainCarve" %>
                  <i  style="color:#8a9906;" class="fa fa-circle"></i>
                <% when "National Franchisee(NF)" %>
                  <i  style="color:#990707;" class="fa fa-circle"></i>
                <% when "Master Franchisee(MF)" %>
                 <i  style="color:#ef8a07;" class="fa fa-circle"></i>
                <% when "District Franchisee(DF)" %>      
                <i  style="color:#46e231;" class="fa fa-circle"></i>
                <% when "Unit Franchisee(UF)" %>      
                <i  style="color:#115be3;" class="fa fa-circle"></i>
               <% end %></p>
             
            <%end%>
          </div>
          </div>
          </div>
          </div>
          </div>
        </div>
  
</div>
</div>
