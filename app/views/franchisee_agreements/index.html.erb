<p id="notice"><%= notice %></p>

<h1>Franchisee Agreements</h1>

<table>
  <thead>
    <tr>
      <th>Franchisee</th>
      <th>Agreement date</th>
      <th>Duration</th>
      <th>Renewal date</th>
      <th>Location</th>
      <th>No of centers</th>
      <th>Advance amount gst</th>
      <th>Balance amount gst</th>
      <th>No of installment</th>
      <th>Center address</th>
      <th>City</th>
      <th>State</th>
      <th>Pincode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @franchisee_agreements.each do |franchisee_agreement| %>
      <tr>
        <td><%= franchisee_agreement.franchisee %></td>
        <td><%= franchisee_agreement.agreement_date %></td>
        <td><%= franchisee_agreement.duration %></td>
        <td><%= franchisee_agreement.renewal_date %></td>
        <td><%= franchisee_agreement.location %></td>
        <td><%= franchisee_agreement.no_of_centers %></td>
        <td><%= franchisee_agreement.advance_amount_gst %></td>
        <td><%= franchisee_agreement.balance_amount_gst %></td>
        <td><%= franchisee_agreement.no_of_installment %></td>
        <td><%= franchisee_agreement.center_address %></td>
        <td><%= franchisee_agreement.city %></td>
        <td><%= franchisee_agreement.state %></td>
        <td><%= franchisee_agreement.pincode %></td>
        <td><%= link_to 'Show', franchisee_agreement %></td>
        <td><%= link_to 'Edit', edit_franchisee_agreement_path(franchisee_agreement) %></td>
        <td><%= link_to 'Destroy', franchisee_agreement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Franchisee Agreement', new_franchisee_agreement_path %>
